{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "gateway",
	"main": "src/index.ts",
	"compatibility_date": "2026-02-19",
	"observability": {
		"enabled": true,
	},
	"compatibility_flags": ["nodejs_compat"],
	"services": [
		{
			"binding": "UPSTREAM_ORIGIN",
			"service": "origin",
		},
		{
			"binding": "EXTERNAL_INTERCEPTOR",
			"service": "custominterceptor",
		},
	],
}
